<resources>
    <style name="AlerDialogStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppThemeYesil" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryYesil</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkYesil</item>
        <item name="colorAccent">@color/colorAccentYesil</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppThemeSari" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimarySari</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkSari</item>
        <item name="colorAccent">@color/colorAccentSari</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>



</resources>
